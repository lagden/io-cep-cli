{
  "name": "io-cep-cli",
  "version": "1.0.0",
  "description": "Search address using zip code through Correios",
  "keywords": [
    "cli-app",
    "cli",
    "cep",
    "io-cep",
    "search",
    "zipcode",
    "brasil"
  ],
  "bug": {
    "url": "https://github.com/lagden/io-cep/issues",
    "email": "lagden@gmail.com"
  },
  "license": "MIT",
  "author": {
    "name": "Thiago Lagden",
    "email": "lagden@gmail.com",
    "url": "http://lagden.in"
  },
  "main": "cli.js",
  "preferGlobal": true,
  "bin": {
    "cep": "cli.js"
  },
  "repository": "lagden/io-cep-cli",
  "scripts": {
    "pretest": "xo",
    "test": "mocha --reporter spec --require should",
    "start": "watch xo"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "devDependencies": {
    "mocha": "*",
    "should": "*",
    "watch": "^0.16.0",
    "xo": "^0.10.0"
  },
  "dependencies": {
    "chalk": "^1.1.0",
    "cli-table": "^0.3.1",
    "commander": "^2.8.1",
    "io-cep": "^0.5.3"
  },
  "xo": {
    "envs": [
      "es6",
      "node"
    ],
    "esnext": true,
    "rules": {
      "arrow-parens": [2, "as-needed"],
      "prefer-reflect": [2, {
        "exceptions": ["delete"]
      }],
      "new-cap": [2, {
        "newIsCap": true,
        "capIsNew": false
      }]
    }
  }
}
