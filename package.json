{
  "name": "io-cep-cli",
  "version": "2.1.0",
  "description": "Busca por informações de uma localidade através do endereço ou CEP utilizando os Correios",
  "keywords": [
    "cli-app",
    "cli",
    "cep",
    "io-cep",
    "search",
    "zipcode",
    "brasil"
  ],
  "bug": {
    "url": "https://github.com/lagden/io-cep/issues",
    "email": "lagden@gmail.com"
  },
  "license": "MIT",
  "author": {
    "name": "Thiago Lagden",
    "email": "lagden@gmail.com",
    "url": "http://lagden.in"
  },
  "files": [
    "cli.js",
    "README.md",
    "license",
    "man"
  ],
  "preferGlobal": true,
  "bin": {
    "correios": "./cli.js"
  },
  "man": "./man/correios.1",
  "repository": "lagden/io-cep-cli",
  "scripts": {
    "man": "mkdir -p man && remark doc/*.?.md",
    "test": "ava"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "devDependencies": {
    "ava": "0.17.0",
    "remark-cli": "2.1.0",
    "remark-man": "4.0.0",
    "xo": "0.17.1"
  },
  "dependencies": {
    "chalk": "1.1.3",
    "cli-table3": "0.5.0",
    "commander": "2.9.0",
    "io-cep": "2.2.1"
  },
  "xo": {
    "envs": [
      "es6"
    ],
    "semicolon": false
  },
  "remarkConfig": {
    "output": "man/correios.1",
    "plugins": {
      "remark-man": {
        "name": "correios",
        "date": "2017-01-06",
        "manual": "correios manual",
        "version": "2.0.0"
      }
    }
  }
}
