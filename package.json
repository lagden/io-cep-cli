{
  "name": "io-cep-cli",
  "version": "0.1.1",
  "description": "Search address using zip code through Correios",
  "license": "MIT",
  "repository": "lagden/io-cep-cli",
  "preferGlobal": true,
  "author": {
    "name": "Thiago Lagden",
    "email": "lagden@gmail.com",
    "url": "lagden.in"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "bin": {
    "cep": "cli.js"
  },
  "files": [
    "cli.js"
  ],
  "keywords": [
    "cli-app",
    "cli",
    "cep",
    "io-cep",
    "search",
    "zipcode",
    "brasil"
  ],
  "scripts": {
    "pretest": "xo && babel --stage 1 --modules common --no-comments es6 --out-dir ./",
    "test": "mocha --compilers js:babel/register --reporter spec --require should",
    "prepublish": "npm test",
    "start": "watch xo es6 -d -u"
  },
  "xo": {
    "envs": [
      "es6",
      "node"
    ],
    "esnext": true,
    "ignores": [
      "cli.js"
    ],
    "rules": {
      "arrow-parens": [2, "as-needed"],
      "prefer-reflect": [2, {"exceptions": ["delete"]}],
      "new-cap": [2, {
        "newIsCap": true,
        "capIsNew": false
      }]
    }
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "mocha": "*",
    "should": "*",
    "watch": "^0.16.0",
    "xo": "^0.9.0"
  },
  "dependencies": {
    "chalk": "^1.1.0",
    "cli-table": "^0.3.1",
    "commander": "^2.8.1",
    "io-cep": "^0.4.4"
  }
}
